{
  "name": "parcel-static-site-starter",
  "version": "1.0.0",
  "description": "A simple toolchain to build your simple static website in the right way",
  "main": "index.js",
  "scripts": {
    "reset": "rimraf dist && rimraf .cache",
    "start": "rimraf dist && parcel \"src/**/*.html\"",
    "build": "rimraf dist && parcel build \"src/**/*.html\" --public-url ./"
  },
  "author": "Naman Gupta",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^9.6.1",
    "parcel-bundler": "^1.12.4",
    "postcss-modules": "^4.3.1",
    "rimraf": "^3.0.2",
    "sass": "^1.54.1",
    "typescript": "^5.8.2"
  }
}
